{
    "name": "just-record",
    "type": "module",
    "version": "0.0.0",
    "private": true,
    "description": "recorder",
    "author": "seemr",
    "license": "MIT",
    "keywords": [
        "electron",
        "rollup",
        "vite",
        "vue3",
        "vue"
    ],
    "main": "dist-electron/main/index.js",
    "debug": {
        "env": {
            "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
        }
