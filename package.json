  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@iconify/json": "^2.2.180",
    "cors": "^2.8.5",
    "fabric": "^5.3.0",
    "fluent-ffmpeg": "^2.1.2",
    "pinia": "^2.1.7",
    "unocss": "^0.58.4",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@types/cors": "^2.8.17",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@vitejs/plugin-vue": "^5.0.1",
    "electron": "^29.1.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "naive-ui": "^2.37.3",
    "tree-kill": "^1.2.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-vue-devtools": "^7.0.24",
    "vue": "^3.4.1",
    "vue-tsc": "^1.8.27"
  }
