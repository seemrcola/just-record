        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^2.6.3",
        "@iconify/json": "^2.2.180",
        "@vitejs/plugin-vue": "^5.0.1",
        "dayjs": "^1.11.10",
        "dexie": "^4.0.4",
        "electron": "^30.0.1",
        "electron-builder": "^24.9.1",
        "eslint": "^8.56.0",
        "pinia": "^2.1.7",
        "sass": "^1.72.0",
        "tree-kill": "^1.2.2",
        "typescript": "^5.3.3",
        "unocss": "^0.58.4",
        "vite": "^5.0.10",
        "vite-plugin-electron": "^0.28.7",
        "vite-plugin-electron-renderer": "^0.14.5",
        "vite-plugin-vue-devtools": "^7.0.24",
        "vue": "^3.4.1",
        "vue-router": "^4.3.0",
        "vue-tsc": "^1.8.27"
